[sub_resource type="GDScript" id=1]

script/source = "extends Position2D

var init_scale = get_scale()
export (String) var player = \"_1\"

func _ready():
	if player == null:
		player = \"_1\"
func _input(event):
	var right = false
	var left = false
	if event is InputEventJoypadMotion:
		if event.get_axis() == 0:
			if int(event.axis_value) > 0:
				right = true
			elif int(event.axis_value) < 0:
				left = true
	if event.is_action_pressed(\"left\" + player) or left:
		set_scale(Vector2(init_scale.x * -1, init_scale.y))
	elif event.is_action_pressed(\"right\" + player) or right:
		set_scale(init_scale)"

